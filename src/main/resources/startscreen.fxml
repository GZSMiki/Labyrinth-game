<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="startscreen.StartScreenController">
   <children>
      <Label alignment="CENTER" focusTraversable="false" layoutX="160.0" layoutY="70.0" prefHeight="58.0" prefWidth="279.0" text="The Labyrinth" textAlignment="CENTER">
         <font>
            <Font name="Garamond" size="40.0" />
         </font>
      </Label>
      <Button fx:id="playButton" layoutX="250.0" layoutY="231.0" mnemonicParsing="false" onAction="#switchToGame" prefHeight="35.0" prefWidth="100.0" text="Play">
         <font>
            <Font name="JetBrains Mono Bold" size="16.0" />
         </font>
      </Button>
      <Button fx:id="resultButton" layoutX="250.0" layoutY="293.0" mnemonicParsing="false" onAction="#switchToResults" prefHeight="35.0" prefWidth="100.0" text="Results">
         <font>
            <Font name="JetBrains Mono Bold" size="16.0" />
         </font>
      </Button>
      <Label alignment="CENTER" layoutX="237.0" layoutY="144.0" prefHeight="25.0" prefWidth="130.0" text="Enter username:" textAlignment="CENTER">
         <font>
            <Font name="JetBrains Mono Regular" size="14.0" />
         </font>
      </Label>
      <TextField fx:id="username" alignment="CENTER" layoutX="225.0" layoutY="174.0" prefHeight="30.0" prefWidth="150.0" promptText="username">
         <font>
            <Font name="JetBrains Mono Regular" size="12.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
