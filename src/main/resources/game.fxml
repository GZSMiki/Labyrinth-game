          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </GridPane>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="600.0">
         <children>
            <Label prefHeight="34.0" prefWidth="83.0" text="Moves:">
               <font>
                  <Font name="JetBrains Mono Bold" size="21.0" />
               </font>
            </Label>
            <TextField fx:id="numberOfMovesField" alignment="CENTER" disable="true" prefHeight="35.0" prefWidth="80.0" text="0">
               <font>
                  <Font name="JetBrains Mono Regular" size="21.0" />
               </font>
            </TextField>
         </children></HBox>
   </children>
