<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="700.0" maxWidth="700.0" minHeight="700.0" minWidth="700.0" prefHeight="700.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.LabyrinthController">
   <children>
      <Button fx:id="checkWinButton" minHeight="50.0" minWidth="50.0" mnemonicParsing="false" onAction="#winButtonClicked" text="Win" textAlignment="CENTER" textFill="RED">
         <VBox.margin>
            <Insets bottom="10.0" left="265.0" top="20.0" />
         </VBox.margin>
         <font>
            <Font name="JetBrains Mono Regular" size="12.0" />
         </font></Button>
      <GridPane fx:id="grid" alignment="CENTER" maxHeight="550.0" maxWidth="550.0" minHeight="550.0" minWidth="550.0" prefHeight="550.0" prefWidth="550.0" stylesheets="@game.css">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </GridPane>
      <AnchorPane prefHeight="150.0" prefWidth="650.0">
         <children>
            <Button alignment="CENTER" layoutX="526.0" layoutY="17.0" mnemonicParsing="false" prefHeight="34.0" prefWidth="127.0" text="Restart">
               <font>
                  <Font name="JetBrains Mono Regular" size="14.0" />
               </font>
            </Button>
            <Label alignment="CENTER" focusTraversable="false" layoutX="23.0" layoutY="21.0" prefHeight="26.0" prefWidth="67.0" text="Player:">
               <font>
                  <Font name="JetBrains Mono Regular" size="14.0" />
               </font>
            </Label>
            <Label layoutX="252.0" layoutY="17.0" prefHeight="34.0" prefWidth="83.0" text="Moves:">
               <font>
                  <Font name="JetBrains Mono Bold" size="21.0" />
               </font>
            </Label>
            <TextField fx:id="numberOfMovesField" alignment="CENTER" disable="true" focusTraversable="false" layoutX="370.0" layoutY="12.0" prefHeight="35.0" prefWidth="80.0" text="0">
               <font>
                  <Font name="JetBrains Mono Regular" size="21.0" />
               </font>
            </TextField>
            <Label fx:id="usernameLabel" focusTraversable="false" layoutX="98.0" layoutY="21.0" prefHeight="26.0" prefWidth="107.0">
               <font>
                  <Font name="JetBrains Mono Regular" size="14.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</VBox>
